      <!-- This is added to filter through the version file to keep it up-to-date -->
    <profiles>
      <profile>
        <id>attach-branch-classifier</id>
        <activation>
          <property>
            <name>branch</name>
          </property>
        </activation>
        <build>
<!-- Add a classifier -> input a branch name to the deployed package -->
          <plugins>
            <plugin>
              <groupId>org.codehaus.mojo</groupId>
              <artifactId>build-helper-maven-plugin</artifactId>
              <version>3.6.1</version>
              <executions>
                <execution>
                  <id>attach-artifacts</id>
                  <phase>package</phase>
                  <goals>
                    <goal>attach-artifact</goal>
                  </goals>
                  <configuration>
                    <skip>${branch}</skip>
                    <artifacts>
                      <artifact>
                        <file>${project.build.directory}/${project.build.finalName}.jar</file>
                        <type>jar</type>
                        <classifier>${branch}</classifier>
                      </artifact>
                    </artifacts>
                  </configuration>
                </execution>
              </executions>
            </plugin>
          </plugins>
        </build>
      </profile>
    </profiles>

